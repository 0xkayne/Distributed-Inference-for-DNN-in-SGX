<!-- Please refer to User's Guide for the explanation of each field -->
<EnclaveConfiguration>
  <ProdID>0</ProdID>
  <ISVSVN>1</ISVSVN>
  
  <!-- Stack Configuration (SGX2 EDMM) -->
  <StackMaxSize>0x2000000</StackMaxSize>        <!-- 32MB max stack -->
  
  <!-- Heap Configuration for EDMM - increased for dynamic allocation -->
  <HeapMaxSize>0x40000000</HeapMaxSize>         <!-- 1GB max heap for EDMM -->
  
  <!-- Thread Control Structure -->
  <TCSNum>4</TCSNum>
  <TCSPolicy>1</TCSPolicy>
  <DisableDebug>0</DisableDebug>
  
  <!-- Misc Select and Mask - for SGX2 EDMM features -->
  <!-- Set MiscSelect[0]=1 and MiscMask[0]=0 to enable dynamic features -->
  <MiscSelect>1</MiscSelect>
  <MiscMask>0xFFFFFFFE</MiscMask>
</EnclaveConfiguration>
